	"scripts": {
		"test": "jest --verbose",
		"dev": "wrangler dev src/index.ts",
		"deploy": "wrangler publish src/index.ts",
		"db:list": "wrangler d1 list",
		"db:create": "wrangler d1 create notifications",
		"db:create:t": "wrangler d1 create notifications_testnet",
		"db:create:d": "wrangler d1 create notifications_devnet",
		"db:schema": "wrangler d1 execute notifications --file schemas/schema.sql",
		"db:schema:t": "wrangler d1 execute notifications_testnet --file schemas/schema.sql",
		"db:schema:d": "wrangler d1 execute notifications_devnet --file schemas/schema.sql"
	},
	"dependencies": {
		"expo-server-sdk": "3.6.0",
		"hono": "^2.5.7"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20221111.1",
		"@types/jest": "^29.2.3",
		"esbuild": "^0.15.15",
		"esbuild-jest": "^0.5.0",
		"jest": "^29.3.1",
		"jest-environment-miniflare": "^2.11.0",
		"wrangler": "^2.4.4"
	}
